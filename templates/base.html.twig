<body>
  
    {# <div id="preloder">
        <div class="loader"></div>
    </div> #}
  <main class="container-fluid">
    <header class="header-section">
        <div class="container-fluid">
            <div class="inner-header">
                <div class="logo">
                    <a href="{{ path ('home')}}"><img src="{{ asset ('images/icons/safar-logo.png')}}" alt="" style="max-height:150px;width:auto;"></a>
                </div>
                <div class="header-right">
                    <img src="{{ asset ('images/icons/search.png')}}" alt="" class="search-trigger">
              
                    <a href="#">
                        <img src="{{ asset ('images/icons/bag.png') }}" alt="">
                        <span>2</span>
                    </a>
                </div>
                <div class="user-access">
                      {% if app.user %}
                    <a href="{{ path ('mon_compte')}}">
                      {{ app.user.prenom }}
                    </a> |                    <a href="{{ path ('app_logout') }}">
                        <i class="fa-solid fa-arrow-right-from-bracket"></i>
                    </a>
                       {% else %}
                      <a href="{{ path ('app_login') }}">
                      Connexion
                      {# <i class="fa-regular fa-user"></i> #}
                        <img src="{{ asset ('images/icons/man.png') }}" alt="">
                      </a>
                    {% endif %}
                    {# <a href="#">Register</a> #}
                    {# <a href="#" class="in">Sign in</a> #}
                </div>
                <nav class="main-menu mobile-menu">
                    <ul>
                        <li><a class="active" href="{{ path ('home')}}">Accueil</a></li>
                        <li><a href="{{ path ('categories')}}">Boutique</a>
                            <ul class="sub-menu">
                                <li><a href="{{ path ('produits')}}">Nos produits</a></li>
                                <li><a href="{{ path ('cart')}}">Panier</a></li>
                                <li><a href="#">Check out</a></li>
                            </ul>
                        </li>
                        <li><a href="{{ path ('quisomme')}}">A propos</a></li>
                        <li><a href="#">Blog</a></li>
                        <li><a href="{{ path ('contact')}}">Contact</a></li>
                    </ul>
                </nav>
            </div>
        </div>
    </header>
    {# NAV BAR #}
    
    {# CAROUSEL #}
    {# {% if block('carousel') is defined %}
    
      
    {% endif %} #}
    {# FIN CAROUSEL #}

    {% block body %}{% endblock %}
  
</div>

</main>
